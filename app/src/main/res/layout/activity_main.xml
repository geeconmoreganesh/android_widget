<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Button"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/toast_btn2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Toast"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/radio_btn3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Radio Button"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/check_btn4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="CheckBox"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/toggle_btn5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Toggle Button"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/switch_btn6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Switch Button"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/image_btn7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Image Button"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/spinner_btn8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Spinner"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/rating_btn9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Rating Bar"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/seek_btn10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Seek Bar"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/progress_btn11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Progress Bar"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/search_btn12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Search View Button"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/alert_btn13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Alert Dailog"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/date_btn14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Date picker"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/time_btn15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Time picker"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/optionMenu_btn16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Option Menu"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/popUpMenu_btn17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="PopUp Menu"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/bottomNavigation_btn18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Bottom Navigation"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/custombottomNavigationBtn19"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Custom Bottom Navigation"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/navigationDrawerBtn20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Navigation Drawer"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/customnavigationDrawerBtn21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=" Custom Navigation Drawer"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/recyclerViewBtn22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Recycler Viiew"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/Implicit_Intent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Implicit Intent"></androidx.appcompat.widget.AppCompatButton>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/Explicit_Intent"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Explicit Intent">
            </androidx.appcompat.widget.AppCompatButton>
        </LinearLayout>
    </ScrollView>
</LinearLayout>